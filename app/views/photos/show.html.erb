

<div class="container bg_white p-5">

  <div clas="row">
    <div class="col-12">
      <p id="notice"><%= notice %></p>

      <p>
        <strong>Album:</strong>
        <%= @photo.album_id %>
      </p>

      <p>
        <strong>Title:</strong>
        <%= @photo.title %>
      </p>

      <p>
        <strong>Description:</strong>
        <%= @photo.description %>
      </p>

      <%= link_to 'Edit', edit_photo_path(@photo) , class: "btn btn-dark" %> |
      <%= link_to 'Back', photos_path , class: "btn btn-dark" %>
    </div>
  </div>

</div>