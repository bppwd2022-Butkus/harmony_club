<div class="container bg_white p-5">

  <div class="row">
    <h1 col-12 my-3>Our Sponsors</h1>
    <% @sponsors.each do |money| %>
      <div class="col-lg-1 col-md-2 col-sm-4 col-12 my-3"><%= link_to image_tag(money.photo, class: "w-100"), money.website, target: :_blank %></div>
    <%end%>
  </div>

</div>